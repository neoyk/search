select max(latency),min(latency),min(time),max(time) from ipv4dnsperf where pagesize!=0 and id=1 and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)<=30 and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)>=0 select latency from ipv4dnsperf where pagesize!=0 and id=1 and token='original' and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)<=30 and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)>=0  order by time\nselect latency from ipv4dnsperf where pagesize!=0 and id=1 and token='smartdns' and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)<=30 and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)>=0  order by time\nselect latency from ipv4dnsperf where pagesize!=0 and id=1 and token='smartdns' and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)<=30 and TO_DAYS('2014-06-18 01:10:07')-TO_DAYS(time)>=0  order by time\n